{
  "_manifest": {
    "urn": "urn:fxsl:agent-bootstrap:analyst-config:1.0.0",
    "type": "bootstrap_config"
  },
  "allowed_kb": [
    "urn:fxsl:kb:xanpan-agents-metodologia",
    "urn:fxsl:kb:stack-llm-arquitectura",
    "urn:fxsl:kb:swarm-ops-metodologia",
    "urn:fxsl:kb:chapter0-operador-solitario"
  ],
  "sandbox": true,
  "tools": {
    "allow": [
      "analysis",
      "metrics_read",
      "kb_read"
    ],
    "deny": [
      "filesystem_write",
      "code_execution",
      "git",
      "web_search",
      "deploy",
      "config_write"
    ]
  },
  "sub_agents": {},
  "limits": {
    "tier_deep_analysis": "T2",
    "no_goodhart": true,
    "require_source_per_metric": true,
    "require_date_per_metric": true,
    "no_fabricated_data": true,
    "stale_threshold_days": 5,
    "min_cycles_for_trend": 2
  },
  "model_routing": {
    "tier_default": "T1"
  }
}
