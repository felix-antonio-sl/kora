{
  "_manifest": {
    "urn": "urn:fxsl:agent-bootstrap:pensador-generador-config:1.0.0",
    "type": "bootstrap_config"
  },
  "allowed_kb": [
    "urn:fxsl:kb:fx-tensiones"
  ],
  "sandbox": {
    "mode": "isolated"
  },
  "tools": {
