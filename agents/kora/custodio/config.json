{
  "_manifest": {
    "urn": "urn:kora:agent-bootstrap:custodio-config:1.0.0",
    "type": "bootstrap_config"
  },
  "allowed_kb": [
    "urn:kora:kb:agent-spec-md",
    "urn:kora:kb:gobernanza",
    "urn:kora:kb:md-spec",
    "urn:kora:kb:spec-md"
  ],
  "sandbox": {
    "mode": "permissive"
  },
  "tools": {
    "allow": [
      "repo_health",
      "catalog_sync",
      "urn_resolve",
      "intake_pipeline",
      "git_status",
      "filesystem_scan",
      "file_write"
    ],
    "deny": [
      "web_search",
      "code_execution"
    ]
  },
  "sub_agents": {
    "max_depth": 1,
    "max_concurrent": 2
  },
  "limits": {
    "policy_flags": {
      "require_confirmation_on_write": true,
      "require_confirmation_on_delete": true
    },
    "quotas": {
      "max_write_per_turn": 5
    }
  }
}
